APP_ABI      := arm64-v8a armeabi-v7a
APP_STL      := c++_shared
APP_PLATFORM := android-24
APP_CPPFLAGS += -std=c++11 -O3 -DQNN_API="__attribute__((visibility(\"default\")))"
APP_LDFLAGS  += -nodefaultlibs -lc -lm -ldl -lgcc

